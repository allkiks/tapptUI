@model GetAdminDashboardDownloadSerializer

    <div class="container-fluid relative animatedParent animateOnce my-3">

        <div class="row row-eq-height my-3 mt-3 ">
            <div class="col-md-4 col-sm-4 ">
                <a href="/Admin/TotalWspList">
                    <div class="card no-b mb-3 ">
                        <div class="card-body mygreentwo text-white">
                            <div class="d-flex justify-content-between align-items-center">
                                <div><i class="icon-people text-blue s-18"></i></div>
                                <div><span class="text-success"></span></div>
                            </div>
                            <div class="text-center">
                                <div class="s-48 my-3 font-weight-lighter">
                                    @Model.WSPCount
                                </div>
                                Total Number Of Water Service Providers
                            </div>
                            <div class="text-center">
                                <div class="s-48 my-3 font-weight-lighter mb-4"></div>

                            </div>
                        </div>
                    </div>
                </a>
            </div>
            <div class="col-md-4 col-sm-4">
                <a href="Admin/MeterList">
                    <div class="card no-b mb-3">
                        <div class="card-body mygreentwo text-white">
                            <div class="d-flex justify-content-between align-items-center">
                                <div>
                                    <i class="icon-calculator text-blue s-18"></i>
                                </div>
                                <div>
                                    <span class="badge badge-pill badge-primary"></span>
                                </div>
                            </div>
                            <div class="text-center">
                                <div class="s-48 my-3 font-weight-lighter">@Model.MeterCount</div>
                                Total Number Of Water Meters
                            </div>
                            <div class="text-center">
                                <div class="s-48 my-3 font-weight-lighter mb-4"></div>

                            </div>
                        </div>
                    </div>
                </a>
            </div>



            <div class="col-md-4 col-sm-4">
                <a href="Admin/TodaysConsumption">
                    <div class="card no-b mb-3">
                        <div class="card-body mygreentwo text-white">
                            <div class="d-flex justify-content-between align-items-center">
                                <div><i class="icon-calculator s-18"></i></div>

                            </div>
                            <div class="text-center">
                                <div class="s-48 my-3 font-weight-lighter mb-5">

                                    <h1>@Model.TodaysConsumption m<sup>3</sup></h1>

                                </div>
                                Today's Total Consumption
                            </div>
                            <div class="text-center">
                                (Cubic Meters)
                            </div>
                        </div>
                    </div>
                </a>

            </div>
        </div>


        <div class="row row-eq-height my-3 mt-3">

            <div class="col-md-4 col-sm-4">
                <a href="Admin/TransmittingCurrent">
                    <div class="card no-b mb-3">
                        <div class="card-body mygreentwo text-white">
                            <div class="d-flex justify-content-between align-items-center">
                                <div><i class="icon-calculator s-18"></i></div>

                            </div>
                            <div class="text-center">
                                <div class="s-48 my-3 font-weight-lighter">@Model.TransmittingCurrent24Hours</div>
                                Water Meters Transmitting
                            </div>
                            <div class="text-center">
                                (Current 24 Hours From 12 AM Midnight EAT)
                            </div>
                        </div>
                    </div>
                </a>
            </div>

            <div class="col-md-4 col-sm-4">
                <a href="Admin/TransmittingPast24">
                    <div class="card no-b mb-3">
                        <div class="card-body mygreentwo  text-white">
                            <div class="d-flex justify-content-between align-items-center">
                                <div><i class="icon-calculator s-18"></i></div>

                            </div>
                            <div class="text-center">
                                <div class="s-48 my-3 font-weight-lighter">@Model.TransmittingLast24Hours</div>

                                Water Meters Transmitting

                            </div>
                            <div class="text-center">
                                (Past 24 Hours To 12 AM Midnight EAT)
                            </div>
                        </div>
                    </div>
                </a>
            </div>


            <div class="col-md-4 col-sm-4">
                <a href="/Admin/NotTransmitting">
                    <div class="card no-b mb-3">
                        <div class="card-body red darken-2 text-white">
                            <div class="d-flex justify-content-between align-items-center">
                                <div><i class="icon-calculator s-18"></i></div>

                            </div>
                            <div class="text-center">
                                <div class="s-48 my-3 font-weight-lighter">@Model.NotTransmitting</div>
                                Water Meters Not Transmitting
                            </div>
                            <div class="text-center">
                                (Past 24 Hours To 12 PM Midnight EAT)
                            </div>
                        </div>
                    </div>
                </a>
            </div>

        </div>

        <div class="card-body white">
            <div class="embed-responsive embed-responsive-16by9">
                <iframe class="embed-responsive-item"
                        src="https://app.powerbi.com/reportEmbed?reportId=b487752e-2008-40e5-aa21-59f58919be69&autoAuth=true&ctid=f04258ce-e4bb-4c2b-9576-b03fd31ccab9&config=eyJjbHVzdGVyVXJsIjoiaHR0cHM6Ly93YWJpLWV1cm9wZS1ub3J0aC1iLXJlZGlyZWN0LmFuYWx5c2lzLndpbmRvd3MubmV0LyJ9"
                        frameborder="0"
                        allowFullScreen="true">
                </iframe>
            </div>
        </div>

        <div class="row row-eq-height mt-3">
            <div class="col-md-12 col-sm-12 col-lg-12">
                <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                    <ol class="carousel-indicators">
                        <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                        <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                        <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                    </ol>
                    <div class="carousel-inner">
                        <div class="carousel-item active">
                            <img class="d-block w-100 " src="~/assets/img/1.png" alt="First slide">
                        </div>
                        <div class="carousel-item">
                            <img class="d-block w-100" src="~/assets/img/2.png" alt="Second slide">
                        </div>
                        <div class="carousel-item">
                            <img class="d-block w-100" src="~/assets/img/3.png" alt="Third slide">
                        </div>
                    </div>
                    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                        <span class="sr-only">Previous</span>
                    </a>
                    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                        <span class="sr-only">Next</span>
                    </a>
                </div>
            </div>
        </div>
    </div>





