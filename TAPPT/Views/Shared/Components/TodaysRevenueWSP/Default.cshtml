@model IEnumerable<TodaysRevenueDownloadSerializer>

    <div class="container-fluid animatedParent animateOnce">
        <div class="tab-content my-3" id="v-pills-tabContent">
            <div class="card no-b ">

                <div class="card-body mt-4">
                    <div class="row my-3 ">
                        <div class="col-md-12">
                            <h1>Todays Revenue</h1>
                            <h6>This page displays todays revenue </h6>
                        </div>

                    </div>
                </div>
            </div>


            <div class="tab-pane animated fadeInUpShort" id="v-pills-buyers" role="tabpanel" aria-labelledby="v-pills-buyers-tab">
            </div>
            <div class="container-fluid animatedParent animateOnce">
                <div class="row my-3">
                    <div class="col-md-12">
                        <div class="card r-0 shadow mb-5">
                            <div class="panel-heading">
                                <div class="panel-body panelsetup">
                                    <h4>LIST OF RETURNED METERS</h4>
                                    <hr />
                                </div>
                            </div>
                            <div class="table-responsive datatablestyle">
                                <table id="tryy" class="table table-striped table-bordered display nowrap" style="width:100%">
                                    <thead>
                                        <tr class="no-b">
                                            <th>
                                                <div class="d-none d-lg-block">Meter Code</div>
                                            </th>
                                            <th>
                                                <div class="d-none d-lg-block">Last Transmitted</div>
                                            </th>
                                            <th>
                                                <div class="d-none d-lg-block">Accumulated FlowRate</div>
                                            </th>

                                            <th>
                                                <div class="d-none d-lg-block">Bill Amount (Kshs)</div>
                                            </th>

                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model)
                                        {
                                            <tr>
                                                <td>
                                                    <div class="d-flex">
                                                        <div>
                                                            <div>
                                                                <strong>@item.meterCode</strong>
                                                            </div>

                                                        </div>
                                                    </div>
                                                </td>

                                                <td>
                                                    <div class="d-none d-lg-block">
                                                        @item.lastTransmitted
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="d-none d-lg-block">
                                                        @item.accumulatedFlowRate
                                                    </div>
                                                </td>


                                                <td>
                                                    <div class="d-none d-lg-block">
                                                        @item.bill
                                                    </div>
                                                </td>


                                                @*<td>
                                                        <a href="/WSP/MeterDetails/@item.Id">
                                                            <button type="button" class="btn btn-primary btn-sm ml-5">View details</button>
                                                        </a>
                                                    </td>*@
                                            </tr>
                                        }
                                    </tbody>
                                    @*<tfoot>
                                            <tr class="no-b">
                                                <th>
                                                    <div class="d-none d-lg-block">Meter Code</div>
                                                </th>
                                                <th>
                                                    <div class="d-none d-lg-block">Geo Cordinates</div>
                                                </th>
                                                <th>
                                                    <div class="d-none d-lg-block">Last Transmitted</div>
                                                </th>
                                                <th>
                                                    <div class="d-none d-lg-block">Accumulated Flow</div>
                                                </th>
                                                <th>
                                                    <div class="d-none d-lg-block">View Details</div>
                                                </th>
                                            </tr>
                                        </tfoot>*@
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>

