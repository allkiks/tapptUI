@model IEnumerable<UtlitiesDownloadSerializer>

    <div class="panel panel-default">
        <div class="panel-body">

            @if (Model.Any())

            {
            <div class="container-fluid animatedParent animateOnce">
                <div class="card no-b r-0 shadow mb-5 mt-4">

                    <div class="card-body">
                        <div class="row my-3 ">
                            <div class="col-md-12">
                                <h1>
                                    Water Meters Not Transmitting
                                   
                                 
                                </h1>
                                <h6>This page displays Water meters not transmitting (Past 24 Hours To 12 AM Midnight EAT)</h6>
                            </div>

                        </div>
                    </div>
                </div>
                <div class="row my-3">
                    <div class="col-md-12">
                        <div class="card-body white r-0 shadow">
                            <div class="panel-heading text-center">
                                <div class="panel-body panelsetup">
                                    <h4>Returned Meters </h4>
                                    <hr />
                                </div>
                            </div>
                            <div class="table-responsive">
                                <table id="tryy" class="table table-striped table-bordered dataTables-example" style="width:100%">
                                    <thead>
                                        <tr class="no-b">
                                            <th>
                                                <div class="d-none d-lg-block">Water Service Provider</div>
                                            </th>

                                            <th>
                                                <div class="d-none d-lg-block">Meters Not Transmitting (Past 24 Hours To 12 AM Midnight EAT)</div>
                                            </th>

                                            <th>
                                                <div class="d-none d-lg-block">Action</div>
                                            </th>
                                        </tr>
                                    </thead>

                                    <tbody>
                                        @foreach (var item in Model)
                                        {
                                            <tr>

                                                <td>
                                                    <div class="d-none d-lg-block">@item.Name</div>
                                                </td>
                                                <td>
                                                    <div class="d-none d-lg-block">@item.CountNotTransmitting</div>
                                                </td>

                                                <td>
                                                    <a href="/Admin/NotTransmittingList/@item.Id" class=" ml-5 btn btn-primary btn-sm">View Meters</a>
                                                </td>
                                            </tr>
                                        }

                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            }
            else
            {

            }
        </div>
    </div>
