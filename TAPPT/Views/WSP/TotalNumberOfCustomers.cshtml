@model IEnumerable<WSPCustomerDownloadSerializer>
@{
    ViewData["Title"] = "Total Number Of Customers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container-fluid animatedParent animateOnce">
    <div class="tab-content my-3" id="v-pills-tabContent">
        <div class="card no-b r-0 shadow">

            <div class="card-body">
                <div class="row my-3 ">
                    <div class="col-md-12">
                        <h3>Customers</h3>
                    </div>

                </div>
            </div>
        </div>

        <div class="row my-3">
            <div class="col-md-12">
                <div class="card-body white r-0 shadow mb-5">
                    <div class="panel-heading text-center">
                        <div class="panel-body panelsetup">
                            <h4>List Of Returned Customers</h4>
                            <hr />
                        </div>
                    </div>
                    <div class="table-responsive datatablestyle">
                        <table id="tryy" class="table table-striped table-bordered display nowrap" style="width:100%">
                            <thead>
                                <tr class="no-b">
                                   
                                    
                                    <th>
                                        <div class="d-none d-lg-block">Meter Code</div>
                                    </th>
                                    <th>
                                        <div class="d-none d-lg-block">Account Number</div>
                                    </th>
                                    <th>
                                        <div class="d-none d-lg-block">Account Name</div>
                                    </th>
                                   
                                    <th>
                                        <div class="d-none d-lg-block">Phone Number</div>
                                    </th>

                                    <th>
                                        <div class="d-none d-lg-block">Account Status</div>
                                    </th>

                                    @*<th>
                                            <div class="d-none d-lg-block">Acction</div>
                                        </th>*@

                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                       
                                      
                                        <td>
                                            <div class="d-flex">
                                                <div>
                                                    <div>
                                                        <strong>@item.MeterCode</strong>
                                                    </div>

                                                </div>
                                            </div>
                                        </td>
                                        <td>
                                            <div class="d-none d-lg-block">@item.AccountNumber</div>
                                        </td>
                                        <td>
                                            <div class="d-none d-lg-block">@item.Owner</div>
                                        </td>
                                       
                                        <td>
                                            <div class="d-none d-lg-block">
                                                @item.PhoneNumber
                                            </div>
                                        </td>
                                        

                                        <td>
                                            <div class="d-none d-lg-block">
                                                <span class="icon icon-circle s-12  mr-2 text-mygreentwo"></span> @item.AccountStatus
                                            </div>
                                        </td>

                                        @*<td>
                                                <a href="#">
                                                    <button type="button" class="btn btn-primary btn-sm ml-5">EDIT</button>
                                                </a>
                                            </td>*@
                                    </tr>
                                }
                            </tbody>
                            @*<tfoot>
                                    <tr class="no-b">
                                        <th>
                                            <div class="d-none d-lg-block">Meter Code</div>
                                        </th>
                                        <th>
                                            <div class="d-none d-lg-block">Geo Cordinates</div>
                                        </th>
                                        <th>
                                            <div class="d-none d-lg-block">Last Transmitted</div>
                                        </th>
                                        <th>
                                            <div class="d-none d-lg-block">Accumulated Flow</div>
                                        </th>
                                        <th>
                                            <div class="d-none d-lg-block">View Details</div>
                                        </th>
                                    </tr>
                                </tfoot>*@
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>




