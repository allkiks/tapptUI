
@{
    ViewData["Title"] = "Users";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container-fluid animatedParent animateOnce">
    <div class="tab-content my-3" id="v-pills-tabContent">
        <div class="card r-0 shadow no-b ">
            <div class="card-body">
                <div class="row my-3 ">
                    <div class="col-md-12">
                        <h1>User Management</h1>
                        <h6></h6>
                    </div>

                </div>
            </div>
        </div>

    </div>
    <header class="white pt-3 relative shadow">
        <div class="container-fluid">
            <div class="tabs-container">
                <ul class="nav nav-material responsive-tab" id="v-pills-tab" role="tablist">
                   
                    <li>
                        <a class="nav-link active show" id="w3--tab1" data-toggle="tab" href="#w3-tab1" role="tab" aria-controls="tab1" aria-expanded="true" aria-selected="true">
                            <i class="icon icon-user"></i>Operator Admin Users
                        </a>
                    </li>
                    <li>
                        <a class="nav-link" id="w3--tab2" data-toggle="tab" href="#w3-tab2" role="tab" aria-controls="tab2" aria-selected="false"<i class="icon icon-user-circle"></i>Operator Manager Users</a>
                    </li>
                    <li>
                        <a class="nav-link" id="w3--tab3" data-toggle="tab" href="#w3-tab3" role="tab" aria-controls="tab3" aria-selected="false"><i class="icon icon-user-circle"></i>Operator Support Users</a>
                    </li>
                    <li>
                        <a class="nav-link" id="w4--tab4" data-toggle="tab" href="#w4-tab4" role="tab" aria-controls="tab4" aria-selected="false"<i class="icon icon-user-circle"></i>WSP Admin Users</a>
                    </li>
                </ul>

            </div>

        </div>
    </header>
    <div class="container-fluid animatedParent animateOnce my-3 white pt-3 relative shadow">
        <div class="card-body no-p">
            <div class="tab-content my-3">
                <div class="tab-pane show active" id="w3-tab1" role="tabpanel" aria-labelledby="w3-tab1">
                    <div class="panel-body">
                        <div class="table-responsive">

                            <table id="tryy" class="table table-striped table-bordered dataTables-example" style="width:100%">
                                <thead>
                                    <tr class="no-b">
                                        <th>User</th>
                                        <th>Email</th>
                                        <th>Role </th>
                                        <th>Action</th>

                                    </tr>
                                </thead>

                                <tbody>
                                    @foreach (var item in ViewBag.Admins)
                                    {
                                        <tr>

                                            <td>
                                                <div class="d-none d-lg-block">@item.DisplayName</div>
                                            </td>

                                            <td>
                                                <div class="d-none d-lg-block">@item.Email</div>
                                            </td>
                                            <td>
                                                <div class="d-none d-lg-block">@item.RoleName</div>
                                            </td>
                                            <td>
                                                <a href="/Admin/ManageUser/@item.Id"
                                                   class="ml-5 btn btn-primary  btn-sm">Manage User</a>
                                            </td>

                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <div class="tab-pane fade" id="w3-tab2" role="tabpanel" aria-labelledby="w3-tab2">
                    <div class="panel-body">
                        <div class="table-responsive">

                            <table id="tryy" class="table table-striped table-bordered dataTables-example" style="width:100%">
                                <thead>
                                    <tr class="no-b">
                                        <th>User</th>
                                        <th>Email</th>
                                        <th>Role </th>
                                        <th>Action</th>

                                    </tr>
                                </thead>

                                <tbody>
                                    @foreach (var item in ViewBag.Management)
                                    {
                                        <tr>

                                            <td>
                                                <div class="d-none d-lg-block">@item.DisplayName</div>
                                            </td>

                                            <td>
                                                <div class="d-none d-lg-block">@item.Email</div>
                                            </td>
                                            <td>
                                                <div class="d-none d-lg-block">@item.RoleName</div>
                                            </td>
                                            <td>
                                                <a href="/Admin/ManageUser/@item.Id"
                                                   class="ml-5 btn btn-primary  btn-sm">Manage User</a>
                                            </td>

                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <div class="tab-pane fade" text-center mt-3" id="w3-tab3" role="tabpanel" aria-labelledby="w3-tab3">
                    <div class="panel-body">
                        <div class="table-responsive">

                            <table id="tryy" class="table table-striped table-bordered dataTables-example" style="width:100%">
                                <thead>
                                    <tr class="no-b">
                                        <th>User</th>
                                        <th>Email</th>
                                        <th>Role </th>
                                        <th>Action</th>

                                    </tr>
                                </thead>

                                <tbody>
                                    @foreach (var item in ViewBag.Support)
                                    {
                                        <tr>

                                            <td>
                                                <div class="d-none d-lg-block">@item.DisplayName</div>
                                            </td>

                                            <td>
                                                <div class="d-none d-lg-block">@item.Email</div>
                                            </td>
                                            <td>
                                                <div class="d-none d-lg-block">@item.RoleName</div>
                                            </td>
                                            <td>
                                                <a href="/Admin/ManageUser/@item.Id"
                                                   class="ml-5 btn btn-primary  btn-sm">Manage User</a>
                                            </td>

                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <div class="tab-pane fade" text-center mt-3" id="w4-tab4" role="tabpanel" aria-labelledby="w3-tab4">
                    <div class="panel-body">
                        <div class="table-responsive">

                            <table id="tryy" class="table table-striped table-bordered dataTables-example" style="width:100%">
                                <thead>
                                    <tr class="no-b">
                                        <th>User</th>
                                        <th>Email</th>
                                        <th>Role </th>
                                        <th>Action</th>

                                    </tr>
                                </thead>

                                <tbody>
                                    @foreach (var item in ViewBag.WSPAdmins)
                                    {
                                        <tr>

                                            <td>
                                                <div class="d-none d-lg-block">@item.DisplayName</div>
                                            </td>

                                            <td>
                                                <div class="d-none d-lg-block">@item.Email</div>
                                            </td>
                                            <td>
                                                <div class="d-none d-lg-block">@item.RoleName</div>
                                            </td>
                                            <td>
                                                <a href="/Admin/ManageUser/@item.Id"
                                                   class="ml-5 btn btn-primary btn-sm">Manage User</a>
                                            </td>

                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>








